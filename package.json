{
  "name": "supereffective-website",
  "version": "1.0.0",
  "private": true,
  "repository": "git@github.com:supeffective/website.git",
  "license": "MIT",
  "author": "Javi Aguilar https://itsjavi.com",
  "scripts": {
    "build": "pnpm codegen && pnpm build:nextjs && pnpm build:sitemap",
    "build-vercel": "pnpm lint && pnpm migrate:deploy && pnpm build",
    "build:nextjs": "next build",
    "build:sitemap": "next-sitemap",
    "clear-cache": "rm -rf .next .turbo",
    "codegen": "prisma generate",
    "dev": "pnpm dev:prepare && pnpm dev:nextjs",
    "dev-debug": "pnpm dev:prepare && NODE_OPTIONS='--inspect' pnpm dev:nextjs",
    "dev-open": "pnpm open && pnpm dev",
    "dev:db": "pnpm codegen && pnpm studio:dev",
    "dev:nextjs": "PORT=3001 next dev",
    "dev:prepare": "docker-compose up -d && env-cmd pnpm migrate",
    "format": "pnpm pretty && pnpm lint:fix",
    "postinstall": "make postinstall",
    "lint": "tsc --noEmit && next lint --max-warnings=25",
    "lint:fix": "next lint --fix --max-warnings=25",
    "migrate": "pnpm migrate:deploy",
    "migrate:deploy": "pnpm prisma generate && pnpm prisma migrate deploy",
    "migrate:dev": "pnpm prisma generate && pnpm prisma migrate dev",
    "migrate:push": "pnpm prisma generate && pnpm prisma db push",
    "open": "open http://localhost:1080 && open http://localhost:3001",
    "prettier-check": "prettier --check \"**/*.{cjs,mjs,js,ts,tsx,json,yml,yaml,css,scss,md,mdx}\"",
    "pretty": "pnpm pretty:code && pnpm pretty:pkgjsons",
    "pretty:code": "prettier --write \"**/*.{cjs,mjs,js,ts,tsx,json,yml,yaml,css,scss,md,mdx}\"",
    "pretty:pkgjsons": "pnpm sort-package-json",
    "prisma:studio": "DATABASE_URL=$STUDIO_DATABASE_URL DIRECT_DATABASE_URL=$STUDIO_DATABASE_URL prisma studio --port 5555 --browser none",
    "prisma:studio-prod": "./lib/scripts/studio-prod.sh",
    "qa": "pnpm quality-checks",
    "quality-checks": "pnpm codegen && pnpm format && pnpm prettier-check && pnpm lint && pnpm build && pnpm test:ci",
    "start": "PORT=3001 next start",
    "test": "turbo test:all",
    "test:all": "jest",
    "test:ci": "pnpm test -- -- --ci",
    "test:coverage": "pnpm test -- -- --coverage"
  },
  "dependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@next-auth/prisma-adapter": "1.0.7",
    "@prisma/client": "^5.7.1",
    "@supeffective/dataset": "^1.0.0",
    "@supeffective/icons": "^0.8.6",
    "@types/canvas-confetti": "^1.6.4",
    "@types/node": "^20.10.5",
    "@types/nodemailer": "^6.4.14",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/react-modal": "^3.16.3",
    "autoprefixer": "^10.4.16",
    "canvas-confetti": "^1.9.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "glob": "^10.3.10",
    "gray-matter": "^4.0.3",
    "husky": "^8.0.3",
    "lucide-react": "0.302.0",
    "nanoid": "^5.0.4",
    "next": "14.0.4",
    "next-auth": "^4.24.5",
    "next-axiom": "1.1.1",
    "next-sitemap": "^4.2.3",
    "nodemailer": "^6.9.7",
    "postcss-import": "^15.1.0",
    "prettier": "^3.1.1",
    "prisma": "^5.7.1",
    "react": "^18.2.0",
    "react-confetti": "^6.1.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-modal": "^3.16.1",
    "resend": "^2.1.0",
    "sort-package-json": "^2.6.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "turbo": "^1.11.2",
    "typescript": "^5.3.3",
    "usehooks-ts": "^2.9.1",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@swc/core": "^1.3.101",
    "@swc/jest": "^0.2.29",
    "@types/jest": "^29.5.11",
    "chokidar": "^3.5.3",
    "jest": "^29.7.0"
  },
  "packageManager": "pnpm@8.12.1",
  "engines": {
    "node": ">=18.5.0 <21.0.0"
  },
  "prisma": {
    "schema": "lib/prisma/schema.prisma"
  }
}
